pipeline {
  agent any 
  tools {
    maven '/usr/share/maven'
  }
  stages {
    stage ('Initialize') {
      steps {
        sh '''
                    echo "PATH = ${PATH}"
                    echo "M2_HOME = ${M2_HOME}"
            ''' 
      }
    }
  }
  stage ('Build') {
      steps {
      sh 'mvn clean package'
       }
    }
}  
